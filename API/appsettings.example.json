{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },

  "TokenKey": "YOUR_SECURE_JWT_TOKEN_KEY_HERE",
  "ContactSettings": {
    "AdminEmail": "your-admin-email@example.com",
    "FromEmail": "noreply@yourdomain.com",
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": "587",
    "SmtpUser": "${SMTP_USER}",
    "SmtpPass": "${SMTP_PASS}"
  }
}